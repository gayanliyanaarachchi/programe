<aiml version="2.0">
   

  <category>
        <pattern>
		        I need to change my phone number in my insurance *
        </pattern>
		
        <template>
           Certainly. May I know your full name?
       </template>
   </category>
   
   <category>
        <pattern>
		         Jim Brown
		</pattern>
		
        <template>
           Thank you. Could you please provide your policy number? 
		</template>
   </category>
   
   <category>
        <pattern>
		         Policy number is 453778908
        </pattern>
		
